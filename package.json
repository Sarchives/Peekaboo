{
    "name": "peekaboo",
    "version": "1.0.0",
    "private": true,
    "description": "Peekaboo is an open-source Discord bot built on TypeScript that's gaining features ever day.",
        "repository": {
        "type": "git",
        "url": "https://github.com/KojiOdyssey/Peekaboo.git"
    },
    "license": "Apache-2",
    "author": "KojiOdyssey",
    "main": "build/index.js",
    "scripts": {
        "lint": "eslint . --ext .ts,.js",
        "lint:fix": "eslint . --ext .ts,.js --fix",
        "start": "cross-env NODE_ENV=\"production\" node -r ts-node/register -r tsconfig-paths/register .",
        "start:dev": "cross-env NODE_ENV=\"development\" nodemon",
        "build": "tsc",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "^5.2.0",
        "@typescript-eslint/parser": "^5.2.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-plugin-import": "^2.25.2",
        "husky": "^7.0.0",
        "nodemon": "^2.0.14",
        "ts-node": "^10.4.0",
        "tsc": "^2.0.3",
        "tsconfig-paths": "^3.11.0",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "@discordjs/rest": "^0.1.0-canary.0",
        "axios": "^0.24.0",
        "chalk": "^4.1.2",
        "discord-api-types": "^0.24.0",
        "discord.js": "^13.3.0",
        "dotenv": "^10.0.0",
        "glob": "^7.2.0",
        "joi": "^17.4.2"
    }
}